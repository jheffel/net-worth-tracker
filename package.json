{
  "name": "net-worth-tracker-web",
  "version": "1.0.0",
  "description": "Web-based net worth tracker with interactive charts and portfolio management",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\" \"node ./scripts/openDevUrl.js\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install",
    "start": "node server/index.js"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.3",
    "moment": "^2.29.4",
    "multer": "^1.4.5-lts.1",
    "sqlite3": "^5.1.6",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "finance",
    "net-worth",
    "portfolio",
    "tracker",
    "react",
    "nodejs"
  ],
  "author": "Net Worth Tracker",
  "license": "MIT"
}
